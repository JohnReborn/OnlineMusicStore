<script type="text/javascript">
   
   function startCountdown() {
		var now = new Date().getTime();

		$('.countdown-container').each(function(){
			
			if($(this).is('.countdown-end')){
				var end = new Date($(this).data('countdown-end').replace(/\s/, 'T')).getTime();
				$(this).find('.countdown-target').html(getCountdownTimeString(end - now))
			}else{
				var start = $(this).data('countdown-start-timestamp')*1000;
				var length = $(this).data('countdown-length')*60*60*1000;
				var repeating = $(this).data('countdown-repeating');
				var text = $(this).data('countdown-text');
				var time = getCountdownTime(start,now,length,repeating);
				$(this).find('.countdown-target').html(text.replace('%time%', getCountdownTimeString(time)))
			}
			
		});

	}

	function getCountdownTime(start, now, length, repeating){
		var time = 0;
		if(repeating){
			time = length - ((now - start) % length);
		}else{
			time = length - (now - start);
		}

		if(time < 0) time = 0;

		return time;
	}

	function getCountdownTimeString(timestamp){
		if(timestamp == null) timestamp = 0;
      var d = Math.floor(timestamp / (1000 * 60 * 60 * 24));
      var h = Math.floor((timestamp % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
      var m = Math.floor((timestamp % (1000 * 60 * 60)) / (1000 * 60));
      var s = Math.floor((timestamp % (1000 * 60)) / 1000);
		var text = "";

		if(d > 0) text = '<time><b>'+d+'</b><span>days</span></time>';
			text += '<time><b>'+h+'</b><span>hrs</span></time>';
			text += '<time><b>'+m+'</b><span>min</span></time>';
			text += '<time><b>'+s+'</b><span>sec.</span></time>';
		
		return text;
   }
	
	var countdownTimerId;

	startCountdown();
	
   documentReady(function() {
		startCountdown();
		
      countdownTimerId = setInterval(function() {
			startCountdown();
      },1000);
   });
   
</script>  

