        <script>
            function newsletterModalInit(open = false) {
                var interactions = parseInt(readCookie('newsletter-visit-interactions')) || 0;

                if (readCookie('newsletter-visit-time') == null) {
                    var date = new Date();
                    createCookie('newsletter-visit-time', date.getTime(), 30);
                }

                if (interactions >= 2 || checkTimeCondition() || open) {
                    if (readCookie('newsletter-modal-in') == null) {
                        //Ošetření proti zobrazení při přesměrování stránky
                        setTimeout(function () {
                            $('#newsletter-modal-in').modal('show');
                            createCookie('newsletter-modal-in', 1, 30);
                        }, 2000);
                    }else if(readCookie('newsletter-sent') == null){
                        $('.newsletter-modal-button').show();
                    }
                }
            }

            // Kontrolu času
            function checkTimeCondition() {
                var visitTime = parseInt(readCookie('newsletter-visit-time')) || 0;
                var currentTime = new Date().getTime();
                var delay = 28 * 1000; // 90 sekund

                var elapsedTime = currentTime - visitTime;

                // zda uplynulo alespoň 90 sekund od návštěvy
                return elapsedTime >= delay;
            }

            var interactionElements = document.querySelectorAll('button, a, img');
            interactionElements.forEach(function (element) {
                element.addEventListener('click', function () {
                    var interactions = parseInt(readCookie('newsletter-visit-interactions')) || 0;
                    var newVal = interactions + 1;
                    createCookie('newsletter-visit-interactions', newVal, 30);
                    if (newVal >= 2) {
                        newsletterModalInit();
                    }
                });
            });
        </script>
