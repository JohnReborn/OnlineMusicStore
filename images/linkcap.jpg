<script>
documentReady(function (){
	var showInfo = readCookie('cookies_set') === null;

	if (showInfo){
		$('#accept_cookies').show();
	}

	$('.cookie-submit').click(function(event) {
		event.preventDefault();

		if(typeof analysisScripts !== 'undefined'){
			var i = 1;
			$(analysisScripts).each(function(){
				var el = this;
				if($(this).is('script') && $(this).attr('src') != undefined){
					$('head').append(el);
				}else{
					setTimeout(function(){
						$('.counters').append(el);
					}, 100 + 20*i);
					i++;
				}
			});
		}
		if(typeof marketingScripts !== 'undefined'){
			var i = 1;
			$(marketingScripts).each(function(){
				var el = this;
				if($(this).is('script') && $(this).attr('src') != undefined){
					$('head').append(el);
				}else{
					setTimeout(function(){
						$('.counters').append(el);
					},100 + 20*i);
					i++;
				}
			});
		}

		$.ajax({
			method: 'get',
			url: '/ajax/accept-cookies/',
			data: {
				'cookies-analysis': 1,
				'cookies-marketing': 1
			},
			success: function() {
				$('#accept_cookies').fadeOut();
				cookieRunScripts();
			}
		});
		
		if(typeof gtag !== 'undefined'){
			gtag('consent', 'update', {
				'ad_storage': 'granted',
				'analytics_storage': 'granted',
				'functionality_storage': 'granted',
				'personalization_storage': 'granted',
				'ad_user_data': 'granted',
				'ad_personalization': 'granted',
				'security_storage': 'granted'
			});
		}
	});


	if(!showInfo && document.location.pathname == '/cookies/'){
		$('#cookies-text').hide();
		$('#cookies_buttons').hide();
		$('#cookies_setting').show();
		$('#accept_cookies').show();
	}
});

function cookieToggleSettings(){
	$('#cookies_setting').toggle();
	$('#cookies-text').hide();
	$('#cookies_buttons').toggle();	
}

function saveCookieSettings(){
	var analysis = $('#cookies-analysis').is(':checked') ? 1:0;
	var marketing = $('#cookies-marketing').is(':checked') ? 1:0;

	if(analysis && typeof analysisScripts !== 'undefined'){
		$('body').append(analysisScripts);
	}
	if(marketing && typeof marketingScripts !== 'undefined'){
		$('body').append(marketingScripts);
		if(typeof gtag !== 'undefined'){
			gtag('consent', 'update', {
				'ad_storage': 'granted',
				'analytics_storage': 'granted',
				'functionality_storage': 'granted',
				'personalization_storage': 'granted',
                'ad_user_data': 'granted',
                'ad_personalization': 'granted',
				'security_storage': 'granted'
			});
		}
	}

	$.ajax({
		method: 'get',
		url: '/ajax/accept-cookies/',
		data: {
			'cookies-analysis': analysis,
			'cookies-marketing': marketing
		},
		success: function() {}
	});

	$('#accept_cookies').fadeOut();
	cookieRunScripts();
}

function denyCookies(){
	$.ajax({
		method: 'get',
		url: '/ajax/accept-cookies/',
		data: {
			'cookies-analysis': 0,
			'cookies-marketing': 0,
		},
		success: function() {}
	});

	$('#accept_cookies').fadeOut();
	cookieRunScripts();
}

function cookieRunScripts(){
	if(typeof newsletterModalInit !== 'undefined'){
		setTimeout(newsletterModalInit, 1000);
	}
	
	if(typeof geopopupInit !== 'undefined'){
		setTimeout(geopopupInit, 5000);
	}
}
</script>  
      
   
	    

		 

